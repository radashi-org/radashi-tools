{
  "name": "radashi-helper",
  "version": "0.1.11",
  "type": "module",
  "description": "Help with managing your own Radashi",
  "author": "Alec Larson",
  "license": "MIT",
  "homepage": "https://radashi.js.org/your-own-radashi",
  "repository": {
    "type": "git",
    "url": "https://github.com/radashi-org/radashi-tools.git",
    "directory": "packages/radashi-helper"
  },
  "main": "dist/cli.cjs",
  "types": "dist/cli.d.cts",
  "bin": {
    "radashi": "bin/radashi-helper.js"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cli.d.cts",
        "default": "./dist/cli.cjs"
      },
      "types": "./dist/cli.d.ts",
      "default": "./dist/cli.js"
    },
    "./esbuild": {
      "require": {
        "types": "./dist/esbuild/plugin.d.cts",
        "default": "./dist/esbuild/plugin.cjs"
      },
      "types": "./dist/esbuild/plugin.d.ts",
      "default": "./dist/esbuild/plugin.js"
    },
    "./vitest": {
      "require": {
        "types": "./dist/vitest/plugin.d.cts",
        "default": "./dist/vitest/plugin.cjs"
      },
      "types": "./dist/vitest/plugin.d.ts",
      "default": "./dist/vitest/plugin.js"
    }
  },
  "files": ["dist"],
  "keywords": ["radashi", "cli"],
  "scripts": {
    "prepublishOnly": "pnpm -s build",
    "build": "tsup --clean",
    "dev": "tsup --clean --watch --sourcemap",
    "lint": "tsc -p . && tsc -p test",
    "format": "biome check --fix --unsafe",
    "test": "vitest"
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "debug": "^4.3.5",
    "esbuild": "^0.23.0",
    "fast-glob": "^3.3.2",
    "kleur": "^4.1.5",
    "prompts": "^2.4.2",
    "tinyspawn": "^1.3.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.24.8",
    "@types/debug": "^4.1.12",
    "@types/prompts": "^2.4.9",
    "cac": "^6.7.14",
    "escalade": "^3.1.2",
    "exec": "link:src/exec",
    "glob-regex": "^0.3.2",
    "p-lazy": "^4.0.0",
    "prompts": "^2.4.2",
    "radashi": "12.2.0-beta.af825f4",
    "string-argv": "^0.3.2",
    "strip-ansi": "^7.1.0",
    "type-fest": "^4.22.0",
    "vite": "^5.3.4",
    "vitest": "^2.0.5"
  }
}
