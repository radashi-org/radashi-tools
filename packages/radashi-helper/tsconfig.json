    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "dist/tmp",
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true
  }
}
