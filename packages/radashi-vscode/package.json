{
  "name": "radashi-vscode",
  "displayName": "Radashi",
  "description": "Commands for manaing your own Radashi",
  "version": "0.1.0",
  "publisher": "aleclarson",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": ["Other"],
  "repository": {
    "type": "git",
    "url": "https://github.com/radashi-org/radashi-vscode"
  },
  "type": "module",
  "contributes": {
    "commands": [
      {
        "command": "radashi.createFunction",
        "title": "üîß Create Function",
        "category": "Radashi",
        "enablement": "radashi.hasRadashiWorkspace"
      },
      {
        "command": "radashi.moveFunction",
        "title": "üîß Move Function",
        "category": "Radashi",
        "enablement": "radashi.hasRadashiWorkspace"
      },
      {
        "command": "radashi.overrideFunction",
        "title": "üîß Override Function",
        "category": "Radashi",
        "enablement": "radashi.hasRadashiWorkspace"
      },
      {
        "command": "radashi.importFunction",
        "title": "üîß Import Function from PR",
        "category": "Radashi",
        "enablement": "radashi.hasRadashiWorkspace"
      },
      {
        "command": "radashi.searchFunctions",
        "title": "‚ú≥Ô∏è Search Functions",
        "category": "Radashi"
      },
      {
        "command": "radashi.proposeNewFunction",
        "title": "‚ú≥Ô∏è Propose New Function",
        "category": "Radashi"
      },
      {
        "command": "radashi.applyToCoreTeam",
        "title": "üèÜ Apply To Core Team",
        "category": "Radashi"
      },
      {
        "command": "radashi.exploreCoreFunctions",
        "title": "‚ú≥Ô∏è Explore Core Functions",
        "category": "Radashi"
      }
    ],
    "keybindings": [
      {
        "command": "radashi.searchFunctions",
        "key": "ctrl+p",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsup --clean",
    "dev": "tsup --clean --watch --sourcemap",
    "prepackage": "node scripts/pre-package.mjs",
    "package": "cd dist && vsce package"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.75.0",
    "@vscode/vsce": "^2.31.1",
    "algoliasearch": "^4.24.0",
    "esbuild": "^0.23.0",
    "p-memoize": "^7.1.1",
    "radashi": "12.2.0-beta.af825f4",
    "tsup": "^8.1.2",
    "typescript": "^5.5.4",
    "yaml": "^2.5.0"
  },
  "dependencies": {
    "execa": "9.3.1",
    "fast-glob": "3.3.2",
    "radashi-helper": "^0.1.0"
  }
}
